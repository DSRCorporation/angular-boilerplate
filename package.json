  "scripts": {
    "version-update-w": "git checkout -B version-inc && npm --no-git-tag-version version %CURR_VERSION_CMD% && git add package.json && git commit -m \"Version Inc NPM\" && bower --offline version %CURR_VERSION_CMD% && git push origin version-inc && npm run version-merge-w",
    "version-merge-w": "git checkout develop && git merge --no-ff version-inc && git push origin develop && git branch -D version-inc && git push origin :version-inc && git tag -d v%npm_package_version% && git fetch origin && git checkout master && git merge --no-ff develop && git push origin master && git tag %npm_package_version% && git push origin %npm_package_version%",
    "version-update-b": "git checkout -B version-inc && npm --no-git-tag-version version $CURR_VERSION_CMD && git add package.json && git commit -m \"Version Inc NPM\" && bower --offline version $CURR_VERSION_CMD && git push origin version-inc && npm run version-merge-b",
    "version-merge-b": "git checkout develop && git merge --no-ff version-inc && git push origin develop && git branch -D version-inc && git push origin :version-inc && git tag -d v$npm_package_version && git fetch origin && git checkout master && git merge --no-ff develop && git push origin master && git tag $npm_package_version && git push origin $npm_package_version",
    "version-patch": "set CURR_VERSION_CMD=patch && npm run version-update-w",
    "version-minor": "set CURR_VERSION_CMD=minor && npm run version-update-w",
    "version-major": "set CURR_VERSION_CMD=major && npm run version-update-w",
    "version-patch-b": "export CURR_VERSION_CMD=patch && npm run version-update-b",
    "version-minor-b": "export CURR_VERSION_CMD=minor && npm run version-update-b",
    "version-major-b": "export CURR_VERSION_CMD=major && npm run version-update-b",
    "upgrade-dependencies": "ncu -a && yarn install",
    "start": "./node_modules/.bin/webpack-dev-server --colors --open --config webpack.dev.js",
    "start-prod": "./node_modules/.bin/webpack-dev-server --colors --open --config webpack.prod.js",
    "build": "./node_modules/.bin/webpack --config webpack.prod.js",
    "lint": "./node_modules/.bin/standard"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "angular",
      "_"
    ]
  }
}
